{
  "name": "@tile-generator/app-engine",
  "description": "App engine server for tilegenerator.",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "prepare": "npm run gcp-build",
